<?xml version="1.0"?>
<package format="2">
  <name>bitbots_hcm</name>
  <version>1.0.0</version>
  <description>The bitbots_hcm package provides a state machine which controlls the current state of the robot.
  It will perform falling and standing-up handling by its own. It will also decide which other nodes can write motor
  goal positions.</description>

  <maintainer email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>

  <author email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</author>
  <author email="info@bit-bots.de">Hamburg Bit-Bots</author>

  <license>MIT</license>


  <buildtool_depend>catkin</buildtool_depend>

  <depend>rospy</depend>

  <depend>std_msgs</depend>
  <depend>humanoid_league_msgs</depend>
  <depend>bitbots_bringup</depend>
  <depend>dynamic_reconfigure</depend>
  <depend>bitbots_msgs</depend>
  <depend>wolfgang_description</depend>
  <depend>wolfgang_animations</depend>
  <depend>wolfgang_moveit_config</depend>
  <depend>dynamic_stack_decider</depend>
  <depend>humanoid_league_speaker</depend>
  <depend>humanoid_base_footprint</depend>

  <export>
    <bitbots_documentation>
      <language>python3</language>
      <status>tested_integration</status>
    </bitbots_documentation>
  </export>
</package>
