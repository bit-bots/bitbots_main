<?xml version="1.0"?>
<package format="2">
  <name>bitbots_ik</name>
  <version>1.0.0</version>
  <description>This package provides an analytic IK solver for robots with a Darwin-OP like kinematic chain and a solver which uses BioIK.</description>

  <maintainer email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>

  <author email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</author>
  <author email="info@bit-bots.de">Hamburg Bit-Bots</author>

  <license>BSD</license>


  <buildtool_depend>catkin</buildtool_depend>

  <depend>roscpp</depend>
  <depend>std_msgs</depend>
  <depend>rosconsole</depend>
  <depend>eigen_conversions</depend>
  <depend>tf</depend>
  <depend>bio_ik</depend>
  <depend>tf_conversions</depend>
  <depend>moveit_core</depend>
  <depend>moveit_ros_move_group</depend>
  <depend>moveit_ros_planning</depend>
  <depend>moveit_ros_planning_interface</depend>
  <depend>geometry_msgs</depend>

  <export>
    <bitbots_documentation>
      <status>tested_robot</status>
      <language>c++</language>
    </bitbots_documentation>
  </export>
</package>
