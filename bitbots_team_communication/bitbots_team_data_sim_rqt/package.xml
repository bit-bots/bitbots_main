<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>bitbots_team_data_sim_rqt</name>
  <version>2.0.0</version>
  <description>
    A Python GUI plugin to send Team Data messages.
  </description>

  <maintainer email="git@flova.de">Florian Vahl</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>

  <author email="info@bitbots.de">Lea Wedmann</author>

  <license>MIT</license>

  <exec_depend>bitbots_hcm</exec_depend>
  <exec_depend>python3-pyqt5</exec_depend>
  <exec_depend>python3-rospkg</exec_depend>
  <exec_depend>rclpy</exec_depend>
  <exec_depend>rqt_gui_py</exec_depend>
  <exec_depend>rqt_gui</exec_depend>
  <exec_depend>std_srvs</exec_depend>

  <export>
    <architecture_independent/>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
    <build_type>ament_python</build_type>
  </export>
</package>
