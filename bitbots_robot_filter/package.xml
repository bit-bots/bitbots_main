<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>bitbots_robot_filter</name>
  <version>0.0.0</version>
  <description>A simple model that keeps track of the robots on the field.</description>
  <maintainer email="git@flova.de">Florian Vahl</maintainer>
  <license>MIT</license>

  <depend>bitbots_tf_listener</depend>
  <depend>geometry_msgs</depend>
  <depend>humanoid_league_msgs</depend>
  <depend>python3-numpy</depend>
  <depend>rclpy</depend>
  <depend>ros2_numpy</depend>
  <depend>soccer_vision_3d_msgs</depend>
  <depend>soccer_vision_attribute_msgs</depend>
  <depend>std_msgs</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>tf2_ros</depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
