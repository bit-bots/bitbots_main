<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>bitbots_utils</name>
    <version>2.3.1</version>
    <description>The bitbots_utils package is a collection of util classes used in many ROS packages from
        the Hamburg Bit-Bots.
    </description>

    <maintainer email="7sell@informatik.uni-hamburg.de">Finn-Thorben Sell</maintainer>
    <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>

    <license>MIT</license>
    
    <author email="info@bit-bots.de">Hamburg Bit-Bots</author>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <depend>bitbots_docs</depend>
    <depend>python3-yaml</depend>
    <depend>python3-transforms3d</depend>
    <depend>demo_nodes_cpp</depend>
    <depend>python3-numpy</depend>
    <depend>geometry_msgs</depend>
    <depend>ros2_numpy</depend>

    <export>
        <bitbots_documentation>
            <status>unknown</status>
        </bitbots_documentation>
        <build_type>ament_cmake</build_type>
    </export>
</package>
