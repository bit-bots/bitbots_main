<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>bitbots_auto_test</name>
  <version>1.2.1</version>
  <description>The bitbots_auto_test automatically runs tests in simulation to measure performance.</description>

  <maintainer email="0krupp@informatik.uni-hamburg.de">Cornelius Krupp</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>

  <license>MIT</license>
  
  <author email="0krupp@informatik.uni-hamburg.de">Cornelius Krupp</author>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>bitbots_docs</depend>
  <depend>bitbots_msgs</depend>
  <depend>rclpy</depend>
  <depend>std_msgs</depend>
  <depend>tf_transformations</depend>
  <depend>game_controller_hl</depend>

  <test_depend>python3-pytest</test_depend>

  <export>
    <bitbots_documentation>
      <language>python3</language>
      <status>untested</status>
    </bitbots_documentation>
    <build_type>ament_python</build_type>
  </export>

  <exec_depend>rosidl_default_runtime</exec_depend>
  </package>
