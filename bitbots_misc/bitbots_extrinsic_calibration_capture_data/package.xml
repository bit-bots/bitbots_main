<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>bitbots_extrinsic_calibration_capture_data</name>
  <version>0.0.0</version>
  <description>A tiny package used to capture data for automatic extrinsic calibration.</description>
  <maintainer email="paer-wiegmann@gmx.de">par</maintainer>
  <license>MIT</license>

  <depend>rclpy</depend>
  <depend>bitbots_msgs</depend>
  <exec_depend>bitbots_motion</exec_depend>
  <exec_depend>bitbots_vision</exec_depend>
  <exec_depend>bitbots_head_mover</exec_depend>
  <exec_depend>ros2launch</exec_depend>
  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
