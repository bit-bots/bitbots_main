<?xml version="1.0"?>
<package format="2">
  <name>bitbots_transform</name>
  <version>1.0.0</version>
  <description>bitbots_transform provides a node to transform from a pixel in an image to real world coordinates.
    CameraInfo and the TF-Tree needs to be available for it to properly work</description>

  <maintainer email="5guelden@informatik.uni-hamburg.de">Jasper GÃ¼ldenstein</maintainer>

  <license>TODO</license>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>roscpp</exec_depend>
  <exec_depend>rosconsole</exec_depend>
  <exec_depend>humanoid_league_msgs</exec_depend>
  <exec_depend>message_generation</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>tf2</exec_depend>
  <exec_depend>tf_conversions</exec_depend>
  <exec_depend>message_generation</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>

  <export>
    <bitbots_documentation>
      <language>python2</language>
      <status>tested_robot</status>
    </bitbots_documentation>
    <rosdoc config="rosdoc.yaml"/>
  </export>
</package>
