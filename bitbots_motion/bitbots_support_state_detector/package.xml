<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>bitbots_support_state_detector</name>
  <version>1.0.13</version>
  <description>The bitbots_support_state_detector package. It is used to detect the support state of the robot (meaning which foot is on the ground).</description>

  <maintainer email="7vahl@informatik.uni-hamburg.de">Florian Vahl</maintainer>

  <license>MIT</license>

  <author email="7vahl@informatik.uni-hamburg.de">Florian Vahl</author>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>biped_interfaces</depend>
  <depend>bitbots_docs</depend>
  <depend>bitbots_msgs</depend>
  <depend>bitbots_utils</depend>
  <depend>rclcpp</depend>
  <depend>generate_parameter_library</depend>

  <export>
    <bitbots_documentation>
      <status>tested_robot</status>
      <language>c++</language>
    </bitbots_documentation>
    <build_type>ament_cmake</build_type>
  </export>

  <buildtool_depend>ament_cmake</buildtool_depend>
</package>
